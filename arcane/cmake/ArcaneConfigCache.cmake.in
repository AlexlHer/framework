# Macro pour positionner une variable si elle ne l'est pas déjà
macro(arcane_set_if_unset)
  if(NOT ${ARGV0})
    message(VERBOSE "SET: ${ARGV0} to ${ARGV1}")
    set(${ARGV0} "${ARGV1}")
  endif()
endmacro()

set(ARCANE_CMAKE_PREFIX_PATH "@CMAKE_PREFIX_PATH@")
