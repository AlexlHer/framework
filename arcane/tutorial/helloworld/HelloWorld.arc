<?xml version='1.0' encoding='ISO-8859-1'?>
<case codeversion="1.0" codename="HelloWorld" xml:lang="en">
	<arcane>
		<title>Exemple Arcane de l'affichage de Hello World!</title>
		<!-- sélectionner la boucle en temps de l'application  -->
	</arcane>

	<mesh>
		<meshgenerator>
			<sod>
				<x>100</x>
				<y>5</y>
				<z>5</z>
			</sod>
		</meshgenerator>
	</mesh>
</case>
